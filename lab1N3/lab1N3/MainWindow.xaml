        <Style x:Key="ToolBarVerticalOverflowButtonStyle2" TargetType="{x:Type ToggleButton}">
            <Setter Property="Background" Value="{StaticResource ToolBarToggleButtonVerticalBackground2}"/>
            <Setter Property="MinHeight" Value="0"/>
