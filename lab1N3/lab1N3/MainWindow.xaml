                                    <theme:SystemDropShadowChrome x:Name="Shdw" Color="Transparent">
                                        <Border x:Name="ToolBarSubMenuBorder" Background="{StaticResource ToolBarSubMenuBackground2}" BorderThickness="1" BorderBrush="{StaticResource ToolBarMenuBorder2}" RenderOptions.ClearTypeHint="Enabled">
